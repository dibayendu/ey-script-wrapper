check process <%= "#{@app_name}_#{@command_name}" %>
  with pidfile /var/run/engineyard/<%= @command_name %>/<%= @app_name %>/<%=@command_name %>.pid
  start program = "/usr/local/bin/<%= "#{@app_name}_#{@command_name}" %>_wrapper <%= @app_name %> start"
  stop program = "/usr/local/bin/<%= "#{@app_name}_#{@command_name}" %>_wrapper <%= @app_name %> stop"
  group <%= "#{@app_name}_#{@command_name}" %>
